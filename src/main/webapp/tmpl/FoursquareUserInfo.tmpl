		<table class="table table-striped">
			<tr>
				<td>Email</td>
				<td>{{contact.email}}</td>
			</tr>			<tr>
				<td>User Name:</td>
				<td>{{lastName}}.{{firstName}}</td>
			</tr>
            <tr>
            <td>Avatar:</td>
                <td><image src="{{photo}}" class="avatar"/></td>
            </tr>
            <td>Gender:</td>
                <td>{{gender}}</td>
            </tr>
		</table>
	</div>
</script>